<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        *{
  margin:0;
  padding:0;
  font-family:  sans-serif;
  
}
html,body{
    width: 100%;
    height: 100%;
}
nav{
   background: rgb(159, 109, 224);
   display: flex;
   justify-content: space-between;
   box-shadow:0 8px 16px rgb(95, 95, 95);
   padding: 0 40px;
}
nav h1{
  border-bottom:4px solid  white;
  color: white;
}
nav ul{
  justify-content: space-around;
  display: flex;
  align-items: center;
  
}
nav li{
 color: white;
  list-style: none;
  padding-right: 30px;
}

nav a{
  color:white;
  text-decoration: none;
  font-weight: bold;
  padding: 5px 8px;
}

nav a:hover{
  color: white;
  border-bottom:4px solid  white;
}

        table{
            border-collapse: collapse;
            width:100%;
            
        }
        th,td{
            text-align: center;
            padding: 8px;
        }
        tr:nth-child(even){
            background-color: #f2f2f2;
        }
        th{
            background-color: rgb(159, 109, 224);
            color: white;
        }
        h2{
            padding: 20px;
            
            border-bottom:4px solid rgb(159, 109, 224);
            
        }
    </style>
    <script>
        var user=[
        {{#each userdata }}
            {
                id:'{{id}}',
                phno:'{{phno}}',
                userid:'{{userid}}',
                name:'{{name}}'
                dept:'{{dept}}'
            }
        {{/each}}
        ];
    </script>
</head>
<body>
     <div class="table-data">
    <nav>
        <img src="https://upload.wikimedia.org/wikipedia/en/f/f8/Amrita-vishwa-vidyapeetham-color-logo.png" height="100px" width="250px" alt="photo">
        <ul>
          <li><a href="admin.hbs"> <b>Home</b></a></li>
          <li><a href="adminfac.hbs"><b>Faculty</b></a></li>
          <li><a href="schedule.hbs"><b>Scheduler</b></a></li>
          <li><a href="adminlogout.hbs">Logout</a></li>
        </ul>
        
    </nav>
    <h2>FACULTY LIST</h2>
    <table>
        <tr>
            <th>Faculty ID</th>
            <th>Full Name</th>
            <th>Email Address</th>
            <th>phno</th>
            <th>DEPT</th>
        </tr>
        {{#each userdata}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.name}}</td>
                <td>{{this.userid}}</td>
                <td>{{this.phno}}</td>
                <td>{{this.dept}}</td>
            </tr>
        {{/each}}
    </table>
    </div>
</body>
</html>